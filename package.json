{
  "name": "happy-bear",
  "version": "1.0.0",
  "description": "Website for Happy Bear Web Design Shop",
  "main": "index.js",
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "serve": "npx @11ty/eleventy --serve autoprefixer",
    "start": "npx gulp && concurrently \"npx gulp watch\" \"npx eleventy --serve\"",
    "production": "NODE_ENV=production npx gulp && NODE_ENV=production npx eleventy"
  },
  "author": "Matt DeCamp",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "autoprefixer": "^10.0.4",
    "concurrently": "^5.3.0",
    "get-google-fonts": "^1.2.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-sass": "^4.1.0",
    "postcss": "^8.1.14",
    "postcss-cli": "^8.3.0",
    "sass": "^1.29.0"
  }
}
